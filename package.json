{
  "name": "AutoAPIGen",
  "displayName": "AutoAPIGen",
  "description": "AutoAPIGen 是 VisualStudio 代码扩展，旨在自动生成 API 函数，提高开发效率。它支持 TypeScript 类型化、查询和路径参数处理，并与 Axios 集成以实现无缝 API 请求。对于希望节省时间和避免重复 API 样板代码的开发人员来说是理想选择。",
  "version": "1.1.0",
  "publisher": "script",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/983033995/AutoAPIGen-Vsix.git"
  },
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "concurrently \"tsc -p ./tsconfig.extension.json --skipLibCheck\" \"vite build\" \"tailwindcss -i ./src/assets/css/main.css -o ./dist/output.css\"",
    "watch": "concurrently \"tsc -w -p ./tsconfig.extension.json --skipLibCheck\" \"vite build --watch\" \"tailwindcss -i ./src/assets/css/main.css -o ./dist/output.css --watch\"",
    "lint": "eslint \"src/**/*.{ts,vue}\"",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "auto-api-gen",
          "title": "AutoAPIGen",
          "icon": "./dist/compiled/default.png"
        }
      ]
    },
    "views": {
      "auto-api-gen": [
        {
          "type": "webview",
          "id": "base-view-sidebar",
          "name": "Auto API Gen Extension",
          "icon": "./dist/compiled/default.png",
          "contextualTitle": "Auto API Gen Extension"
        }
      ]
    },
    "commands": [
      {
        "command": "AutoAPIGen.showConfigPagePanel",
        "title": "showConfigPagePanel"
      },
      {
        "command": "AutoAPIGen.closeConfigPagePanel",
        "title": "closeConfigPagePanel"
      },
      {
        "command": "AutoAPIGen.copyToClipboard",
        "title": "Copy Text to Clipboard"
      }
    ],
    "activationEvents": [
      "onView: base-view-sidebar",
      "onCommand:AutoAPIGen.showConfigPagePanel",
      "onCommand:AutoAPIGen.closeConfigPagePanel",
      "onCommand:AutoAPIGen.copyToClipboard"
    ]
  },
  "dependencies": {
    "@iconify-json/hugeicons": "^1.1.0",
    "@iconify-json/noto": "^1.1.18",
    "@iconify/tailwind": "^1.1.1",
    "@intlify/unplugin-vue-i18n": "^0.8.1",
    "@monaco-editor/loader": "^1.4.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/prettier": "^3.0.0",
    "@vitest/ui": "^0.26.2",
    "@vueuse/core": "^10.11.0",
    "axios": "^1.7.2",
    "eslint": "^8.50.0",
    "fs-extra": "^11.2.0",
    "monaco-editor": "^0.52.0",
    "pinyin-pro": "^3.23.1",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "vue": "^3.2.45",
    "vue-i18n": "9"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.55.3",
    "@arco-plugins/vite-vue": "^1.4.5",
    "@iconify-json/logos": "^1.1.43",
    "@iconify-json/mdi": "^1.1.39",
    "@iconify-json/ph": "^1.1.13",
    "@iconify-json/tabler": "^1.1.117",
    "@iconify-json/vscode-icons": "^1.1.35",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/vue": "^6.6.1",
    "@types/jest": "^29.2.4",
    "@types/node": "^22.9.0",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "concurrently": "^7.6.0",
    "eslint-plugin-vue": "^9.30.0",
    "jsdom": "^20.0.3",
    "tailwindcss": "^3.2.4",
    "typescript": "^5.6.3",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-icons": "^0.14.15",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^2.9.14",
    "vitest": "^0.26.2",
    "vue-tsc": "^2.1.10"
  }
}
